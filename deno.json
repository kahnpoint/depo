{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "npm:react",
    "types": [
      "https://unpkg.com/@types/react@18.2.37/index.d.ts",
      "lume/types.ts"
    ]
  },
  "tasks": {
    "main": "NODE_ENV='development' deno run -A src/main.ts",
    "dev": "deno cache src/main.ts && NODE_ENV='development' deno run -A --watch src/main.ts",
    "build": "deno compile --output ./depo src/main.ts"
  },
  "imports": {
    "cliffy-command": "https://deno.land/x/cliffy@v1.0.0-rc.3/command/mod.ts",
    "lume/": "https://deno.land/x/lume@v2.0.2/",
    "moment": "https://esm.sh/v135/moment",
    "node-html-parser": "https://esm.sh/v135/node-html-parser",
    "deps": "./src/deps.ts"
  }
}
