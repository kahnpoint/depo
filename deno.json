{
  "compilerOptions": {},
  "tasks": {
    "mod": "NODE_ENV='development' deno run -A src/mod.ts",
    "dev": "deno cache src/mod.ts && NODE_ENV='development' deno run -A --watch src/mod.ts",
    "build": "deno compile -A --output /usr/local/bin/depo src/mod.ts"
  },
  "imports": {
    "@/": "./src/",
    "cliffy-command": "https://deno.land/x/cliffy@v1.0.0-rc.3/command/mod.ts",
    "moment": "https://esm.sh/v135/moment@2.30.1",
    "node-html-parser": "https://esm.sh/v135/node-html-parser",
    "deps": "./src/deps.ts",
    "fs": "https://deno.land/std@0.210.0/fs/mod.ts"
  }
}
